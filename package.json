{
  "name": "fastify-multer",
  "description": "Fastify plugin for handling `multipart/form-data`.",
  "version": "2.0.3",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "contributors": [
    "Maksim Sinik <maksim@sinik.it>",
    "Hage Yaapa <captain@hacksparrow.com> (http://www.hacksparrow.com)",
    "Jaret Pfluger <https://github.com/jpfluger>",
    "Linus Unneb√§ck <linus@folkdatorn.se>"
  ],
  "license": "MIT",
  "repository": "https://github.com/fox1t/fastify-multer",
  "keywords": [
    "form",
    "post",
    "multipart",
    "form-data",
    "formdata",
    "fastify",
    "middleware"
  ],
  "dependencies": {
    "@fastify/busboy": "3.1.1",
    "append-field": "^2.0.0",
    "concat-stream": "^2.0.0",
    "fastify-plugin": "5.0.1",
    "mkdirp": "^3.0.1",
    "on-finished": "^2.4.1",
    "type-is": "1.6.18",
    "xtend": "^4.0.2"
  },
  "devDependencies": {
    "@types/concat-stream": "2.0.3",
    "@types/mocha": "10.0.10",
    "@types/node": "22.12.0",
    "@types/on-finished": "^2.3.4",
    "@types/type-is": "^1.6.7",
    "@types/xtend": "4.0.6",
    "fastify": "^5.2.1",
    "form-data": "4.0.1",
    "fs-temp": "2.0.1",
    "mocha": "11.1.0",
    "prettier": "3.4.2",
    "rimraf": "^6.0.1",
    "testdata-w3c-json-form": "~1.0.0",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=10.17.0"
  },
  "files": [
    "lib/",
    "src",
    "typings/"
  ],
  "scripts": {
    "prebuild": "npm run clean-build && npm run lint",
    "build": "tsc -p ./tsconfig.json",
    "clean-build": "rimraf ./lib && mkdir lib",
    "prettier": "prettier --loglevel warn --write \"src/**/*.{ts,tsx}\"",
    "prelint": "npm run prettier",
    "lint": "tslint --fix -c tslint.json -t stylish -p ./tsconfig.json",
    "update": "npx npm-check -u",
    "prepublishOnly": "npm run build",
    "test": "npm run build && mocha --require test/tshook.js ./test/*.ts"
  }
}
